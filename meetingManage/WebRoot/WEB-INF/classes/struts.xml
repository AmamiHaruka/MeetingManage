<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<constant name="struts.objectFactory" value="spring"></constant>
<package name="find" extends="struts-default" namespace="/find">
	<action name="personAction" class="personAction">
	<result name = "success">/find.jsp</result>
	<result name = "fail">/finderror.jsp</result>
	</action>
	<action name= "search" class = "personAction" method="SearchPerson">
	<result name = "success">/find.jsp</result>
	</action>
	<action name = "delete" class="personAction" method="deletePerson">
	<result name = "success">/find.jsp</result>
	</action>
</package>
</struts>    
